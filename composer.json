{
    "name": "girover/tree",
    "description": "Building family tree",
    "keywords": [
        "girover",
        "laravel",
        "tree"
    ],
    "homepage": "https://github.com/girover/tree",
    "license": "MIT",
    "authors": [
        {
            "name": "Majed Girover",
            "email": "girover.mhf@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Girover\\Tree\\": "src",
            "Girover\\Tree\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Girover\\Tree\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Girover\\Tree\\TreeServiceProvider"
            ],
            "aliases": {
                "FamilyTree": "Girover\\Tree\\Facades\\FamilyTree"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
